= form_for @protagnist do |f|
  -if @protagnist.errors.any?
    #error_explanation
      %h2= "#{pluralize(@protagnist.errors.count, "error")} prohibited this protagnist from being saved:"
      %ul
        - @protagnist.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :story_id
    = f.number_field :story_id
  .field
    = f.label :character_id
    = f.number_field :character_id
  .actions
    = f.submit 'Save'
