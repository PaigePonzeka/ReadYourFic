= form_for @story_ship do |f|
  -if @story_ship.errors.any?
    #error_explanation
      %h2= "#{pluralize(@story_ship.errors.count, "error")} prohibited this story_ship from being saved:"
      %ul
        - @story_ship.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :story_id
    = f.number_field :story_id
  .field
    = f.label :ship_id
    = f.number_field :ship_id
  .actions
    = f.submit 'Save'
